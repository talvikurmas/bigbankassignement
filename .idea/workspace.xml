      <find>package com\.bigbank\;\nimport java\.util\.concurrent\.TimeUnit\;\n\nimport cucumber\.api\.java\.en\.And\;\nimport org\.junit\.Assert\;\nimport org\.openqa\.selenium\.By\;\nimport org\.openqa\.selenium\.WebDriver\;\nimport cucumber\.api\.java\.en\.Given\;\nimport cucumber\.api\.java\.en\.Then\;\nimport cucumber\.api\.java\.en\.When\;\nimport com\.bigbank\.locators\.Locators\;\nimport com\.bigbank\.browserhandler\.BrowserHandler\;\n\npublic class CreateBookSteps \{\n\n    public static WebDriver driver\;\n\n    \/\/ Bookstore stuff\n    \@Given\(\&quot;\^I open library webApp\$\&quot;\)\n    public void I_open_library_webApp\(\) throws Throwable \{\n        BrowserHandler bh\;\n        bh \= new BrowserHandler\(\)\;\n        driver \= bh\.getBrowser\(\)\;\n    \}\n    \@And\(\&quot;\^I navigate to create new book page\$\&quot;\)\n    public void I_navigate_to_create_new_book_page\(\) throws Throwable \{\n        driver\.findElement\(By\.linkText\(\&quot;Create new book\&quot;\)\)\.click\(\)\;\n    \}\n\n    \@Then\(\&quot;\^I can see options to create new book\$\&quot;\)\n    public void I_can_see_options_to_create_new_book\(\) throws Throwable \{\n        String exp_message \= \&quot;Create Book\&quot;\;\n        String actual \= driver\.findElement\(By\.xpath\(\&quot;\/\/h1\[contains\(text\(\)\,\'Create Book\'\)\]\&quot;\)\)\.getText\(\)\;\n        Assert\.assertEquals\(exp_message\, actual\)\;\n        Assert\.assertTrue\(driver\.getPageSource\(\)\.contains\(\&quot;Title\&quot;\)\)\;\n        Assert\.assertTrue\(driver\.getPageSource\(\)\.contains\(\&quot;Author\&quot;\)\)\;\n        Assert\.assertTrue\(driver\.getPageSource\(\)\.contains\(\&quot;Summary\&quot;\)\)\;\n        Assert\.assertTrue\(driver\.getPageSource\(\)\.contains\(\&quot;ISBN\&quot;\)\)\;\n    \}\n\n    \@And\(\&quot;\^I input book name\$\&quot;\)\n    public void I_input_book_name\(\) throws Throwable \{\n        Thread\.sleep\(2000\)\;\n        driver\.manage\(\)\.timeouts\(\)\.implicitlyWait\(10\, TimeUnit\.SECONDS\)\;\n        driver\.findElement\(By\.xpath\(Locators\.BookName\)\)\.click\(\)\;\n        driver\.findElement\(By\.xpath\(\&quot;\/\/input\[\@id\=\'title\'\]\&quot;\)\)\.sendKeys\(\&quot;cucumber101\&quot;\)\;\n    \}\n\n    \@And\(\&quot;\^I select author\$\&quot;\)\n    public void I_select_author\(\) throws Throwable \{\n        driver\.manage\(\)\.timeouts\(\)\.implicitlyWait\(10\, TimeUnit\.SECONDS\)\;\n        driver\.findElement\(By\.xpath\(\&quot;\/\/select\[\@id\=\'author\'\]\&quot;\)\)\.sendKeys\(\&quot;King\, Stephan\&quot;\)\;\n    \}\n\n    \@And\(\&quot;\^I input summary\$\&quot;\)\n    public void I_input_summary\(\) throws Throwable \{\n        driver\.findElement\(By\.xpath\(\&quot;\/\/input\[\@id\=\'summary\'\]\&quot;\)\)\.sendKeys\(\&quot;King\, Stephan\&quot;\)\;\n\n    \}\n\n    \@And\(\&quot;\^I input ISBN\$\&quot;\)\n    public void I_input_ISBN\(\) throws Throwable \{\n        driver\.findElement\(By\.xpath\(\&quot;\/\/input\[\@id\=\'isbn\'\]\&quot;\)\)\.sendKeys\(\&quot;123456\&quot;\)\;\n    \}\n\n    \@And\(\&quot;\^I check genre as fiction\&quot;\)\n    public void I_check_genre_as_fiction\(\) throws Throwable \{\n        Thread\.sleep\(1000\)\;\n        driver\.findElement\(By\.xpath\(\&quot;\/\/label\[contains\(text\(\)\,\'Fiction\'\)\]\&quot;\)\)\.click\(\)\;\n        Thread\.sleep\(6000\)\;\n    \}\n\n\n    \@When\(\&quot;\^I press submit\$\&quot;\)\n    public void I_press_submit\(\) throws Throwable \{\n        driver\.findElement\(By\.xpath\(\&quot;\/\/label\[contains\(text\(\)\,\'Fiction\'\)\]\&quot;\)\)\.click\(\)\;\n\n    \}\n\n    \@Then\(\&quot;\^book is saved\$\&quot;\)\n    public void book_is_saved\(\) throws Throwable \{\n        String exp_message \= \&quot;Welcome to your account\. Here you can manage all   of your personal information and orders\.\&quot;\;\n\n    \}\n\n    \@Then\(\&quot;\^created book can be found\$\&quot;\)\n    public void created_book_can_be_found\(\) throws Throwable \{\n        if\(driver\.getPageSource\(\)\.contains\(\&quot;lalala\&quot;\)\)\n        \{\n            System\.out\.print\(\&quot;Book found\&quot;\)\;\n        \}\n\n        else\n        \{\n            System\.out\.print\(\&quot;Book NOT found\&quot;\)\;\n            if \(driver \=\= null\) \{\n\n            \}\n\n\n        \}\n    \}\n\n\}</find>
